{
  "name": "@wosai/chiron-web",
  "version": "1.0.0-beta.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development BABEL_ENV=development webpack-serve --port 3000 --open",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest -u",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack"
  },
  "dependencies": {
    "@wosai/chiron-web-compat": "^1.0.0-beta.0",
    "preact": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.56",
    "@babel/preset-env": "^7.0.0-beta.56",
    "@babel/preset-react": "^7.0.0-beta.56",
    "@babel/preset-typescript": "^7.0.0-beta.56",
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^23.3.1",
    "@types/node": "8.x",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.2",
    "jest-enzyme": "^6.0.2",
    "jest-spec-reporter": "^1.0.4",
    "preact-compat": "^3.18.2",
    "prop-types": "^15.6.2",
    "react": "15.x",
    "react-dom": "15.x",
    "source-map-loader": "^0.2.3",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.2"
  },
  "browserslist": [
    "> 0.05%",
    "not dead"
  ],
  "babel": {
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env",
          [
            "@babel/preset-react",
            {
              "development": true
            }
          ],
          "@babel/preset-typescript"
        ]
      },
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react",
          "@babel/preset-typescript"
        ]
      },
      "production": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "useBuiltIns": "usage"
            }
          ],
          [
            "@babel/preset-react",
            {
              "pragma": "preact.h"
            }
          ],
          [
            "@babel/preset-typescript",
            {
              "jsxPragma": "preact.h"
            }
          ]
        ]
      }
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node",
      "less",
      "scss",
      "css"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./scripts/jest-setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>../../node_modules/jest-enzyme/lib/index.js",
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "transform": {
      "\\.(jpe?g|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/scripts/jest-file-transformer.js",
      "^.+\\.tsx?$": "babel-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.spec.{ts,tsx}",
      "!src/*/style/*.d.ts"
    ],
    "reporters": [
      "default",
      "jest-spec-reporter"
    ]
  }
}
