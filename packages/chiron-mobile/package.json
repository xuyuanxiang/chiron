{
  "name": "chiron-mobile",
  "version": "1.0.0-beta.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development BABEL_ENV=development webpack-serve --port 3000",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production webpack --progress --log-level info"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "preact": "^8.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@types/history": "^4.7.0",
    "@types/node": "8.x",
    "@types/universal-router": "^6.0.0",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^8.0.2",
    "clean-webpack-plugin": "^0.1.19",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "koa-connect": "^2.0.1",
    "koa-router": "^7.4.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "postcss-px2rem": "^0.3.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "universal-router": "^6.0.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.2",
    "webpack-command": "^0.4.1",
    "webpack-serve": "^2.0.2"
  },
  "browserslist": [
    "ios >= 9.3",
    "android >= 4.4"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ],
      [
        "@babel/preset-react",
        {
          "pragma": "h"
        }
      ],
      [
        "@babel/preset-typescript",
        {
          "isTSX": true,
          "jsxPragma": "h",
          "allExtensions": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import"
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": {
                "node": "8.0.0"
              }
            }
          ]
        ]
      }
    }
  }
}
